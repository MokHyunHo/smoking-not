<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
 
<item android:id="@android:id/background">
    <shape>
        <!-- radius background of progressbar -->
        <corners android:radius="2dip" />
        <gradient
                android:startColor="#FF001A"
                android:centerColor="#FF001A"
                android:centerY="0.75"
                android:endColor="#FF001A"
                android:angle="270"
        />
    </shape>
</item>
 
<item android:id="@android:id/secondaryProgress">
    <clip>
        <shape>
            <corners android:radius="0dip" />
            <gradient
                    android:startColor="#FF001A"
                    android:centerColor="#FF001A"
                    android:centerY="0.75"
                    android:endColor="#FF001A"
                    android:angle="270"
            ></gradient>
        </shape>
    </clip>
</item>
<item
    android:id="@android:id/progress"
>
    <clip>
        <shape>
            <!-- radius of secundary progress -->
            <corners
                android:radius="0dip" />
            <gradient
                android:startColor="#006400"
                android:centerColor="#006400"
                android:centerY="0.75"
                android:endColor="#006400"
                android:angle="270" />
        </shape>
    </clip>
</item>
 
</layer-list>